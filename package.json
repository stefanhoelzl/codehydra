{
  "name": "codehydra",
  "version": "0.1.0",
  "description": "Multi-workspace IDE for parallel AI agent development",
  "type": "module",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "svelte-check --tsconfig ./tsconfig.web.json --fail-on-warnings",
    "validate": "pnpm format:check && pnpm lint && pnpm check && pnpm test && pnpm build",
    "validate:fix": "pnpm format && pnpm lint:fix && pnpm check && pnpm test && pnpm build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "patchedDependencies": {
      "systeminformation@5.27.11": "patches/systeminformation@5.27.11.patch"
    }
  },
  "dependencies": {
    "@vscode-elements/elements": "^2.3.1",
    "@vscode/codicons": "^0.0.43",
    "eventsource": "^4.1.0",
    "execa": "^9.6.1",
    "pidtree": "^0.6.0",
    "simple-git": "^3.30.0",
    "systeminformation": "^5.27.11",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "electron": "^39.2.6",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.45.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
